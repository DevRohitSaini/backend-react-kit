    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "eslint": "^7.29.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-babel": "^5.3.1",
    "faker": "^5.5.3",
    "isparta": "^3.0.3",
    "mocha": "^9.0.1",
    "nodemon": "^2.0.19"
  }
