{
  "name": "react-starter-backend",
  "version": "1.0.0",
  "description": "backend code React-starter app",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && babel app -d build",
    "clean": "rm -rf build && mkdir build",
    "start": "nodemon",
    "test": "NODE_ENV=test mocha --recursive --reporter spec --compilers js:babel-register tests",
    "lint": "eslint app",
    "test:watch": "nodemon --exec 'npm run test' tests",
    "coverage": "NODE_ENV=test babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive --reporter spec --compilers js:babel-register tests",
    "validate": "npm run test && npm outdated --depth 0"
  },  
  "keywords": [
    "express",
    "es6",
    "rest",
    "api",
    "boilerplate",
    "mongo"
  ],
  "author": "myexpertise <rohit.saini@myexpertise.io>",
  "license": "MIT",  
  "homepage": "https://github.com/ocaapp/oca-backend",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-stage-0": "^7.8.3",
    "@babel/register": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "convert-excel-to-json": "^1.7.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-paginate": "^1.0.2",
    "fs-extra": "^10.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "moment": "^2.29.4",
    "mongoose": "^5.2.15",
    "mongoose-auto-increment": "^3.0.1",
    "mongoose-sequence": "^5.3.1",
    "morgan": "^1.10.0",
    "msw": "^0.45.0",
    "multer": "^1.4.2",
    "node-google-login": "^1.0.1",
    "nodemailer": "^6.6.2",
    "pify": "^5.0.0",
    "puppeteer": "^19.8.5",
    "randomatic": "^3.1.1",
    "request": "^2.88.2",
    "swissqrbill": "^3.2.0",
